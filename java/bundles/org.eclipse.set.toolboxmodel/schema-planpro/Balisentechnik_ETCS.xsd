                           minOccurs="1"
                           maxOccurs="1"/>
					          <xs:element name="DP_Bezug_Betrieblich" type="nsBalisentechnik_ETCS:CDP_Bezug_Betrieblich"
                           minOccurs="0"
                           maxOccurs="1"/>
					          <xs:element name="DP_ETCS_Adresse" type="nsBalisentechnik_ETCS:CDP_ETCS_Adresse"
                           minOccurs="0"
                           maxOccurs="1">
						            <xs:annotation>
