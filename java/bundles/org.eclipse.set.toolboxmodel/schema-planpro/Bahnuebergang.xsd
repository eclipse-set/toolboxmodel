            </xs:annotation>
			      </xs:element>
			      <xs:element name="Schaltgruppe" type="nsBahnuebergang:TCSchaltgruppe" minOccurs="0"
                     maxOccurs="1">
				        <xs:annotation>
               <xs:documentation xmlns:ppi="http://www.plan-pro.org/modell/PlanProInformation">Zuordnung des Schrankenantriebs zu einer Schaltgruppe.</xs:documentation>
               <xs:appinfo xmlns:ppi="http://www.plan-pro.org/modell/PlanProInformation">
                  <ppi:WorkflowInformation>
                     <ppi:Patternbeschreibung>[1..10]</ppi:Patternbeschreibung>
                  </ppi:WorkflowInformation>
               </xs:appinfo>
            </xs:annotation>
			      </xs:element>
		    </xs:sequence>
	  </xs:complexType>
	  <xs:simpleType name="TFahrbahn_Befestigung_Gleis">
		    <xs:restriction base="nsBasisTypen:TText"/>
	  </xs:simpleType>
	  <xs:simpleType name="TSignalverz_Gewaehlt">
		    <xs:restriction base="nsBasisTypen:TSekunde">
			      <xs:pattern value="(0|[1-9][0-9]{0,4})\.[0-9]{2}"/>
		    </xs:restriction>
	  </xs:simpleType>
