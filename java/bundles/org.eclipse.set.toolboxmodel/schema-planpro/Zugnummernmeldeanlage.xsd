		    <xs:restriction base="xs:string">
			      <xs:enumeration value="sonstige"/>
			      <xs:enumeration value="ZN_B950"/>
			      <xs:enumeration value="ZN_SuB"/>
			      <xs:enumeration value="ZNE_L2000"/>
			      <xs:enumeration value="ZNL_2000"/>
			      <xs:enumeration value="ZNL_800"/>
			      <xs:enumeration value="ZNP_801"/>
			      <xs:enumeration value="ZNS_801"/>
