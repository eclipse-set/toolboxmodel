         <xs:appinfo xmlns:ppi="http://www.plan-pro.org/modell/PlanProInformation">
            <ppi:WorkflowInformation>
               <ppi:ObjectType>LST_OBJECT</ppi:ObjectType>
            </ppi:WorkflowInformation>
         </xs:appinfo>
      </xs:annotation>
		    <xs:complexContent>
			      <xs:extension base="nsBasisobjekte:CBasis_Objekt">
				        <xs:sequence>
					          <xs:element name="ID_ZN" type="nsVerweise:TCID_ZN" minOccurs="1" maxOccurs="1">
						            <xs:annotation>
                     <xs:documentation xmlns:ppi="http://www.plan-pro.org/modell/PlanProInformation">Verweis auf die ZN, zu der das Fortschaltkriterium gehört. </xs:documentation>
                  </xs:annotation>
					          </xs:element>
					          <xs:element name="ID_ZN_Fortschalt_Krit_Start" type="nsVerweise:TCID_ZN_Anzeigefeld"
                           minOccurs="1"
                           maxOccurs="1">
						            <xs:annotation>
                     <xs:documentation xmlns:ppi="http://www.plan-pro.org/modell/PlanProInformation">Verweis auf das ZN Anzeigefeld in der Bedienoberfläche, von dem aus die Zugnummer fortgeschaltet wird. </xs:documentation>
                  </xs:annotation>
					          </xs:element>
					          <xs:element name="ID_ZN_Fortschalt_Krit_Ziel" type="nsVerweise:TCID_ZN_Anzeigefeld"
                           minOccurs="0"
                           maxOccurs="unbounded">
						            <xs:annotation>
                     <xs:documentation xmlns:ppi="http://www.plan-pro.org/modell/PlanProInformation">Verweis auf das ZN Anzeigefeld in der Bedienoberfläche, in das die Zugnummer fortgeschaltet wird. </xs:documentation>
                  </xs:annotation>
					          </xs:element>
					          <xs:element name="ZN_Fortschalt_Krit_Druck"
                           type="nsZugnummernmeldeanlage:CZN_Fortschalt_Krit_Druck"
                           minOccurs="0"
                           maxOccurs="1"/>
					          <xs:element name="ZN_Fortschalt_Krit_Schalt"
                           type="nsZugnummernmeldeanlage:CZN_Fortschalt_Krit_Schalt"
