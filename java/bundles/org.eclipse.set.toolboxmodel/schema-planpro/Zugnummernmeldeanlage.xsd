				        <xs:sequence>
					          <xs:element name="Wert" type="nsZugnummernmeldeanlage:ENUMZN_Anzeigefeld_Loeschkriterium"
                           minOccurs="1"
                           maxOccurs="1"
                           nillable="true"/>
