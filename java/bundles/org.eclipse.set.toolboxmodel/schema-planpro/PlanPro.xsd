            </xs:annotation>
			      </xs:element>
			      <xs:element name="Planung_E_Qualitaetspruefung" type="nsPlanPro:CAkteur_Zuordnung"
                     minOccurs="0"
                     maxOccurs="unbounded">
				        <xs:annotation>
               <xs:documentation xmlns:ppi="http://www.plan-pro.org/modell/PlanProInformation">Abbildung aller notwendigen Informationen zur Dokumentation der Qualitätsprüfung.</xs:documentation>
            </xs:annotation>
			      </xs:element>
			      <xs:element name="Planung_E_Sonstige" type="nsPlanPro:CAkteur_Zuordnung" minOccurs="0"
                     maxOccurs="unbounded">
				        <xs:annotation>
               <xs:documentation xmlns:ppi="http://www.plan-pro.org/modell/PlanProInformation">Abbildung aller notwendigen Informationen zur Dokumentation sonstiger Handlungen.</xs:documentation>
            </xs:annotation>
			      </xs:element>
			      <xs:element name="Planung_E_Uebernahme" type="nsPlanPro:CAkteur_Zuordnung" minOccurs="0"
                     maxOccurs="unbounded">
