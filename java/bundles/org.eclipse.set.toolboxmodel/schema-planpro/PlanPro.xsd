	  <xs:complexType name="CAkteur">
		    <xs:annotation>
         <xs:documentation xmlns:ppi="http://www.plan-pro.org/modell/PlanProInformation">Am PlanPro-Planungsprozess beteiligter Akteur.\r\nIn PlanPro werden der Name und Kontaktdaten des Akteurs erfasst. Einschränkungen hinsichtlich der zulässigen Rolle des Akteurs sind durch entsprechende Attributbezeichnungen und -beschreibungen ersichtlich.\r\nDB-Regelwerk\r\n• Schriftfeldeintrag gemäß Ril 819.0103\r\n</xs:documentation>
         <xs:appinfo xmlns:ppi="http://www.plan-pro.org/modell/PlanProInformation">
            <ppi:WorkflowInformation>
               <ppi:ObjectType>LST_OBJECT</ppi:ObjectType>
            </ppi:WorkflowInformation>
         </xs:appinfo>
      </xs:annotation>
		    <xs:complexContent>
			      <xs:extension base="nsBasisobjekte:CUr_Objekt">
				        <xs:sequence>
					          <xs:element name="Akteur_Allg" type="nsPlanPro:CAkteur_Allg" minOccurs="1" maxOccurs="1"/>
					          <xs:element name="Kontaktdaten" type="nsPlanPro:COrganisation" minOccurs="1" maxOccurs="1">
						            <xs:annotation>
                     <xs:documentation xmlns:ppi="http://www.plan-pro.org/modell/PlanProInformation">Kontaktdaten des Akteurs zur Unternehmenszuordnung.\nDB-Regelwerk\nBisher keine Abbildung als Schriftfeldeintrag gemäß Ril 819.0103.</xs:documentation>
                  </xs:annotation>
					          </xs:element>
				        </xs:sequence>
			      </xs:extension>
		    </xs:complexContent>
	  </xs:complexType>
	  <xs:complexType name="TCDatum_Abschluss_Gruppe">
		    <xs:complexContent>
			      <xs:extension base="nsBasisTypen:CBasisAttribut">
				        <xs:sequence>
					          <xs:element name="Wert" type="xs:date" minOccurs="1" maxOccurs="1" nillable="true"/>
				        </xs:sequence>
			      </xs:extension>
		    </xs:complexContent>
	  </xs:complexType>
