		    <xs:restriction base="xs:string">
			      <xs:enumeration value="Bogen"/>
			      <xs:enumeration value="Blosskurve"/>
			      <xs:enumeration value="Bloss_einfach_geschwungen"/>
			      <xs:enumeration value="Gerade"/>
			      <xs:enumeration value="Klothoide"/>
			      <xs:enumeration value="Km_Sprung"/>
			      <xs:enumeration value="Polylinie"/>
			      <xs:enumeration value="Richtgerade_Knick_am_Ende_200_gon"/>
			      <xs:enumeration value="sonstige"/>
			      <xs:enumeration value="S_Form_einfach_geschwungen"/>
