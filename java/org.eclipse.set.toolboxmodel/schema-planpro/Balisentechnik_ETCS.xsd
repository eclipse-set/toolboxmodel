                           minOccurs="0"
                           maxOccurs="1"/>
					          <xs:element name="ETCS_Signal_TBV" type="nsBalisentechnik_ETCS:CETCS_Signal_TBV"
                           minOccurs="0"
                           maxOccurs="1"/>
					          <xs:element name="ID_ETCS_Gefahrpunkt" type="nsVerweise:TCID_Markanter_Punkt" minOccurs="0"
                           maxOccurs="1">
						            <xs:annotation>
                     <xs:documentation xmlns:ppi="http://www.plan-pro.org/modell/PlanProInformation">Angabe des ETCS-Gefahrpunkts (Danger Point).</xs:documentation>
                  </xs:annotation>
					          </xs:element>
					          <xs:element name="ID_ETCS_Gefahrpunkt_2" type="nsVerweise:TCID_Markanter_Punkt"
                           minOccurs="0"
                           maxOccurs="1">
						            <xs:annotation>
