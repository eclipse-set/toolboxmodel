							              <xs:annotation>
                        <xs:documentation xmlns:ppi="http://www.plan-pro.org/modell/PlanProInformation">Verweis auf ein Signal, das als Ziel-Element zur Beschreibung des Fahrwegteils verwendet wird.</xs:documentation>
                     </xs:annotation>
						            </xs:element>
						            <xs:element name="Ziel_W_Element" type="nsBalisentechnik_ETCS:CZiel_W_Element"
                              minOccurs="0"
                              maxOccurs="1"/>
