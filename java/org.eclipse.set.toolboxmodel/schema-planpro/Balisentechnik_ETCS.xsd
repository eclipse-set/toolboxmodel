	  <xs:complexType name="CETCS_Kante">
		    <xs:annotation>
         <xs:documentation xmlns:ppi="http://www.plan-pro.org/modell/PlanProInformation">Abbildung der Kante des ETCS-spezifischen Knoten-Kanten-Modells auf das topologische PlanPro-Knoten-Kanten-Modell.</xs:documentation>
         <xs:appinfo xmlns:ppi="http://www.plan-pro.org/modell/PlanProInformation">
            <ppi:WorkflowInformation>
               <ppi:ObjectType>LST_OBJECT</ppi:ObjectType>
               <ppi:Untergewerke>ETCS</ppi:Untergewerke>
            </ppi:WorkflowInformation>
         </xs:appinfo>
      </xs:annotation>
		    <xs:complexContent>
			      <xs:extension base="nsBasisobjekte:CBasis_Objekt">
				        <xs:sequence>
					          <xs:element name="Bezeichnung" type="nsBalisentechnik_ETCS:CETCS_Kante_Bezeichnung"
                           minOccurs="1"
                           maxOccurs="1"/>
					          <xs:element name="ID_ETCS_Knoten_A" type="nsVerweise:TCID_ETCS_Knoten_ohne_Proxy"
                           minOccurs="1"
                           maxOccurs="1">
						            <xs:annotation>
                     <xs:documentation xmlns:ppi="http://www.plan-pro.org/modell/PlanProInformation">Verweis auf den ETCS-Knoten am Anfang der Kante.</xs:documentation>
                  </xs:annotation>
					          </xs:element>
					          <xs:element name="ID_ETCS_Knoten_B" type="nsVerweise:TCID_ETCS_Knoten_ohne_Proxy"
                           minOccurs="1"
                           maxOccurs="1">
						            <xs:annotation>
                     <xs:documentation xmlns:ppi="http://www.plan-pro.org/modell/PlanProInformation">Verweis auf den ETCS-Knoten am Ende der Kante.</xs:documentation>
                  </xs:annotation>
					          </xs:element>
					          <xs:element name="ID_RBC" type="nsVerweise:TCID_RBC" minOccurs="1" maxOccurs="unbounded">
						            <xs:annotation>
                     <xs:documentation xmlns:ppi="http://www.plan-pro.org/modell/PlanProInformation">Verweis auf das RBC, in dessen Bereich die ETCS-Kante liegt. Im Bereich der RBC-Grenze ist ggf. eine Mehrfachangabe erforderlich.</xs:documentation>
                  </xs:annotation>
					          </xs:element>
					          <xs:element name="ID_Strecke" type="nsVerweise:TCID_Strecke" minOccurs="0" maxOccurs="1">
						            <xs:annotation>
                     <xs:documentation xmlns:ppi="http://www.plan-pro.org/modell/PlanProInformation">Verweis auf die Strecke, der die STCS-Kante zugeordnet wird. Die Angabe lässt sich durch Auswertung der topologischen Verortung der Strecke(n) sowie der Verortung der ETCS_Knoten ermitteln. Eine Abspeicherung kann zur Erleichterung weiterer Anwendungen (z. B. DiB) erfolgen.</xs:documentation>
                  </xs:annotation>
					          </xs:element>
					          <xs:element name="ID_TOP_Kante" type="nsVerweise:TCID_TOP_Kante_ohne_Proxy" minOccurs="1"
                           maxOccurs="3">
						            <xs:annotation>
                     <xs:documentation xmlns:ppi="http://www.plan-pro.org/modell/PlanProInformation">Verweis auf die TOP-Kanten, die vollständig oder teilweise die ETCS-Kante bilden.</xs:documentation>
                  </xs:annotation>
					          </xs:element>
				        </xs:sequence>
			      </xs:extension>
		    </xs:complexContent>
	  </xs:complexType>
	  <xs:complexType name="TCMassgebende_Neig_Schutzstrecke">
		    <xs:complexContent>
			      <xs:extension base="nsBasisTypen:CBasisAttribut">
