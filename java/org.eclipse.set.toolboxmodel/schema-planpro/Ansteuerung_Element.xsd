	  <xs:complexType name="CTechnik_Standort_Bezeichnung">
		    <xs:annotation>
         <xs:documentation xmlns:ppi="http://www.plan-pro.org/modell/PlanProInformation">Bezeichnung des Technikstandorts</xs:documentation>
      </xs:annotation>
		    <xs:sequence>
