		    <xs:complexContent>
			      <xs:extension base="nsBasisTypen:CBasisAttribut">
				        <xs:sequence>
					          <xs:element name="Wert" type="nsAnsteuerung_Element:ENUMUnterbringung_Befestigung"
                           minOccurs="1"
