                           maxOccurs="unbounded">
						            <xs:annotation>
                     <xs:documentation xmlns:ppi="http://www.plan-pro.org/modell/PlanProInformation">Entsprechender (Lage)Plan oder Beschreibung als Anhang.</xs:documentation>
                  </xs:annotation>
					          </xs:element>
					          <xs:element name="ID_Anhang_UeWeg_Von" type="nsVerweise:TCID_Anhang" minOccurs="0"
                           maxOccurs="unbounded">
						            <xs:annotation>
                     <xs:documentation xmlns:ppi="http://www.plan-pro.org/modell/PlanProInformation">Entsprechender (Lage)Plan oder Beschreibung als Anhang.</xs:documentation>
                  </xs:annotation>
					          </xs:element>
					          <xs:element name="ID_Uebertragungsweg_Nach" type="nsVerweise:TCID_Uebertragungsweg_Nach"
                           minOccurs="1"
                           maxOccurs="1">
						            <xs:annotation>
                     <xs:documentation xmlns:ppi="http://www.plan-pro.org/modell/PlanProInformation">Information, nach wo die Verbindung hergestellt werden soll. Beispiel: Bedien_Bezirk.</xs:documentation>
                  </xs:annotation>
					          </xs:element>
					          <xs:element name="ID_Uebertragungsweg_Von" type="nsVerweise:TCID_Uebertragungsweg_Von"
                           minOccurs="1"
                           maxOccurs="1">
						            <xs:annotation>
                     <xs:documentation xmlns:ppi="http://www.plan-pro.org/modell/PlanProInformation">Information, von wo die Verbindung hergestellt werden soll. Beispiel: ESTW_Zentraleinheit.</xs:documentation>
                  </xs:annotation>
					          </xs:element>
					          <xs:element name="Uebertragungsweg_Art" type="nsAnsteuerung_Element:TCUebertragungsweg_Art"
                           minOccurs="1"
                           maxOccurs="1">
						            <xs:annotation>
                     <xs:documentation xmlns:ppi="http://www.plan-pro.org/modell/PlanProInformation">Verwendung des Übertragungsweges. Beispiel: UZ-BZ-Verbindung.</xs:documentation>
                  </xs:annotation>
					          </xs:element>
					          <xs:element name="Uebertragungsweg_Technik"
                           type="nsAnsteuerung_Element:CUebertragungsweg_Technik"
                           minOccurs="1"
                           maxOccurs="1"/>
				        </xs:sequence>
			      </xs:extension>
		    </xs:complexContent>
