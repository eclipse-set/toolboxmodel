            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType ecore:name="Trasse_Kante_Art_TypeClass" name="TCTrasse_Kante_Art">
      <xs:complexContent>
         <xs:extension base="nsBasisTypen:CBasisAttribut">
            <xs:sequence>
               <xs:element name="Wert"
                            type="nsMedien_und_Trassen:ENUMTrasse_Kante_Art"
                            minOccurs="1"
