   <xs:complexType ecore:name="Trasse_Kante" name="CTrasse_Kante">
      <xs:annotation>
         <xs:documentation>Kante des topologischen Knoten-Kanten-Modells zur Darstellung des Kabelgefäßsystems (Kabeltrasse) oder gleichartiger Medientrassen. Die Trasse Kante ist vom Knoten A zum Knoten B gerichtet und muss immer an zwei Trasse Knoten enden. Der geometrische Verlauf einer Trasse_Kante kann durch eine oder mehrere GEO_Kanten beschrieben werden (siehe auch TOP_Kante).</xs:documentation>
         <xs:appinfo>
            <WorkflowInformation xmlns="http://www.plan-pro.org/modell/PlanProInformation">
               <ObjectType>LST_OBJECT</ObjectType>
               <Untergewerke>ESTW|BÜ</Untergewerke>
            </WorkflowInformation>
         </xs:appinfo>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="nsBasisobjekte:CBasis_Objekt">
            <xs:sequence>
               <xs:element name="ID_Trasse_Knoten_A"
                            type="xs:anyURI"
                            ecore:reference="nsMedien_und_Trassen:CTrasse_Knoten"
                            nillable="true"
                            minOccurs="1"
                            maxOccurs="1">
                  <xs:annotation>
                     <xs:documentation>Verweis auf den Trasse Knoten am Anfang der Kante.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="ID_Trasse_Knoten_B"
                            type="xs:anyURI"
                            ecore:reference="nsMedien_und_Trassen:CTrasse_Knoten"
                            nillable="true"
                            minOccurs="1"
                            maxOccurs="1">
                  <xs:annotation>
                     <xs:documentation>Verweis auf den Trasse Knoten am Ende der Kante.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="Trasse_Kante_Art"
                            type="nsMedien_und_Trassen:TCTrasse_Kante_Art"
                            minOccurs="1"
                            maxOccurs="1">
                  <xs:annotation>
                     <xs:documentation>Art der Medientrasse (z. B. Luft- oder Trogtrasse).</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="Trasse_Nutzer"
                            type="nsMedien_und_Trassen:TCTrasse_Nutzer"
                            minOccurs="1"
                            maxOccurs="unbounded">
                  <xs:annotation>
                     <xs:documentation>Nutzer der Medientrasse, z. B. DB Netz. Eine Trasse kann von mehreren Nutzern beansprucht werden.</xs:documentation>
                     <xs:appinfo>
                        <WorkflowInformation xmlns="http://www.plan-pro.org/modell/PlanProInformation">
                           <ProposedValue>DB_Netz</ProposedValue>
                        </WorkflowInformation>
                     </xs:appinfo>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:simpleType name="ENUMTrasse_Knoten_Art">
      <xs:annotation>
         <xs:documentation>"Schacht, Trassenabzweig"==Abzweig beliebig vieler Trassen;
"Trassenauslass"==Auslass von Kabeln (z. B. zu einem Kabelverteiler), wobei die Verbindung Trasse - Verteiler nicht als eigene Trasse abgebildet wird </xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:string">
         <xs:enumeration ecore:name="ENUMTrasse_Knoten_Art_Schacht" value="Schacht"/>
         <xs:enumeration ecore:name="ENUMTrasse_Knoten_Art_sonstige" value="sonstige"/>
         <xs:enumeration ecore:name="ENUMTrasse_Knoten_Art_Trassenabzweig"
                          value="Trassenabzweig"/>
         <xs:enumeration ecore:name="ENUMTrasse_Knoten_Art_Trassenaenderung"
                          value="Trassenaenderung"/>
         <xs:enumeration ecore:name="ENUMTrasse_Knoten_Art_Trassenauslass"
                          value="Trassenauslass"/>
         <xs:enumeration ecore:name="ENUMTrasse_Knoten_Art_Trassenende" value="Trassenende"/>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType ecore:name="Kabel_Typ_Type" name="TKabel_Typ">
      <xs:restriction base="nsBasisTypen:TText"/>
   </xs:simpleType>
   <xs:simpleType name="ENUMTrasse_Kante_Art">
      <xs:annotation>
         <xs:documentation>"frei"==Kabelverlegung ohne Kabeltiefbau (z. B. Verlegung auf Schotter, Verbindung benachbarter Schaltschränke)</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:string">
         <xs:enumeration ecore:name="ENUMTrasse_Kante_Art_Erdtrasse" value="Erdtrasse"/>
         <xs:enumeration ecore:name="ENUMTrasse_Kante_Art_frei" value="frei"/>
         <xs:enumeration ecore:name="ENUMTrasse_Kante_Art_Kabelgestell_Tunnel"
                          value="Kabelgestell_Tunnel"/>
         <xs:enumeration ecore:name="ENUMTrasse_Kante_Art_Lufttrasse" value="Lufttrasse"/>
         <xs:enumeration ecore:name="ENUMTrasse_Kante_Art_Rohrtrasse" value="Rohrtrasse"/>
         <xs:enumeration ecore:name="ENUMTrasse_Kante_Art_sonstige" value="sonstige"/>
         <xs:enumeration ecore:name="ENUMTrasse_Kante_Art_Trogtrasse" value="Trogtrasse"/>
         <xs:enumeration ecore:name="ENUMTrasse_Kante_Art_Trogtrasse_aufgestaendert"
                          value="Trogtrasse_aufgestaendert"/>
         <xs:enumeration ecore:name="ENUMTrasse_Kante_Art_Trogtrasse_Behelf"
                          value="Trogtrasse_Behelf"/>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType ecore:name="Bezeichnung_Kabel_Type" name="TBezeichnung_Kabel">
      <xs:restriction base="nsBasisTypen:TText">
         <xs:pattern value="[1-9]|1[0-6]"/>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="ENUMKabel_Verteilpunkt_Art">
      <xs:restriction base="xs:string">
         <xs:enumeration ecore:name="ENUMKabel_Verteilpunkt_Art_Kabelschrank"
                          value="Kabelschrank"/>
         <xs:enumeration ecore:name="ENUMKabel_Verteilpunkt_Art_Kabelverteiler"
                          value="Kabelverteiler"/>
         <xs:enumeration ecore:name="ENUMKabel_Verteilpunkt_Art_sonstige" value="sonstige"/>
      </xs:restriction>
   </xs:simpleType>
   <xs:complexType ecore:name="Trasse_Nutzer_TypeClass" name="TCTrasse_Nutzer">
      <xs:complexContent>
         <xs:extension base="nsBasisTypen:CBasisAttribut">
            <xs:sequence>
               <xs:element name="Wert"
                            type="nsMedien_und_Trassen:ENUMTrasse_Nutzer"
                            minOccurs="1"
                            maxOccurs="1"
                            nillable="true"/>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType ecore:name="Kabel_Verteilpunkt_Art_TypeClass"
                    name="TCKabel_Verteilpunkt_Art">
      <xs:complexContent>
         <xs:extension base="nsBasisTypen:CBasisAttribut">
            <xs:sequence>
               <xs:element name="Wert"
                            type="nsMedien_und_Trassen:ENUMKabel_Verteilpunkt_Art"
                            minOccurs="1"
                            maxOccurs="1"
                            nillable="true"/>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType ecore:name="Kabel_Verteilpunkt_Bezeichnung_AttributeGroup"
                    name="CKabel_Verteilpunkt_Bezeichnung">
      <xs:sequence>
         <xs:element name="Bezeichnung_Kabel_Verteilpunkt"
                      type="nsMedien_und_Trassen:TCBezeichnung_Kabel_Verteilpunkt"
                      minOccurs="1"
                      maxOccurs="1">
            <xs:annotation>
               <xs:documentation>Bezeichnung des Kabel-Verteilpunkts.</xs:documentation>
               <xs:appinfo>
                  <WorkflowInformation xmlns="http://www.plan-pro.org/modell/PlanProInformation">
                     <Patternbeschreibung>[1..20]</Patternbeschreibung>
                  </WorkflowInformation>
               </xs:appinfo>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType ecore:name="Kabel_Verteilpunkt" name="CKabel_Verteilpunkt">
      <xs:annotation>
         <xs:documentation>Punktförmige Installation zur Aufteilung bzw. Verschaltung von Kabeln (ohne Intelligenz).</xs:documentation>
         <xs:appinfo>
            <WorkflowInformation xmlns="http://www.plan-pro.org/modell/PlanProInformation">
               <ObjectType>LST_OBJECT</ObjectType>
               <Untergewerke>ESTW|BÜ</Untergewerke>
            </WorkflowInformation>
         </xs:appinfo>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="nsBasisobjekte:CBasis_Objekt">
            <xs:sequence>
               <xs:element name="Bezeichnung"
                            type="nsMedien_und_Trassen:CKabel_Verteilpunkt_Bezeichnung"
                            minOccurs="1"
                            maxOccurs="1"/>
               <xs:element name="ID_Unterbringung"
                            type="xs:anyURI"
                            ecore:reference="nsAnsteuerung_Element:CUnterbringung"
                            nillable="true"
                            minOccurs="1"
                            maxOccurs="1">
                  <xs:annotation>
                     <xs:documentation>Art des Kabel-Verteilpunkts.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="Kabel_Verteilpunkt_Art"
                            type="nsMedien_und_Trassen:TCKabel_Verteilpunkt_Art"
                            minOccurs="1"
                            maxOccurs="1">
                  <xs:annotation>
                     <xs:documentation>Verweis auf die Unterbringung des Kabel-Verteilpunkts. Als Art der Unterbringung ist dabei immer "keine" auszuwählen, da der Kabel-Verteilpunkt selbst die physische Unterbringung definiert.</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType ecore:name="Kabel_Element_AttributeGroup" name="CKabel_Element">
      <xs:sequence>
         <xs:element name="Ader_Reserve"
                      type="nsMedien_und_Trassen:TCAder_Reserve"
                      minOccurs="1"
                      maxOccurs="1">
            <xs:annotation>
               <xs:documentation>Anzahl der Reserveadern. Bsp.: 3x4x1,4 (4).
Die unter Anzahl_Verseilelemente angebbare Aderanzahl muss auch als Reserve angebbar sein (freigeschaltetes Kabel).</xs:documentation>
               <xs:appinfo>
                  <WorkflowInformation xmlns="http://www.plan-pro.org/modell/PlanProInformation">
                     <Patternbeschreibung>[1..200]</Patternbeschreibung>
                  </WorkflowInformation>
               </xs:appinfo>
            </xs:annotation>
         </xs:element>
         <xs:element name="Anzahl_Verseilelemente"
                      type="nsMedien_und_Trassen:TCAnzahl_Verseilelemente"
                      minOccurs="1"
                      maxOccurs="1">
            <xs:annotation>
               <xs:documentation>Anzahl der Verseilelemente. Bsp.: 3x4x1,4.</xs:documentation>
               <xs:appinfo>
                  <WorkflowInformation xmlns="http://www.plan-pro.org/modell/PlanProInformation">
                     <Patternbeschreibung>[1..200]</Patternbeschreibung>
                  </WorkflowInformation>
               </xs:appinfo>
            </xs:annotation>
         </xs:element>
         <xs:element name="Verseilart"
                      type="nsMedien_und_Trassen:TCVerseilart"
                      minOccurs="0"
                      maxOccurs="1">
            <xs:annotation>
               <xs:documentation>Verseilart (4 = Viererverseilung; 1 = adrige Verseilung). Bsp.: 3x4x1,4.</xs:documentation>
               <xs:appinfo>
                  <WorkflowInformation xmlns="http://www.plan-pro.org/modell/PlanProInformation">
                     <Patternbeschreibung>[1,4]</Patternbeschreibung>
                  </WorkflowInformation>
               </xs:appinfo>
            </xs:annotation>
         </xs:element>
         <xs:choice minOccurs="1" maxOccurs="1">
            <xs:element name="Ader_Durchmesser"
                         type="nsMedien_und_Trassen:TCAder_Durchmesser"
                         minOccurs="0"
                         maxOccurs="1">
               <xs:annotation>
                  <xs:documentation>Bei Signalkabeln: Durchmesser der Kabeladern in mm. Bsp.: 3x4x1,4.</xs:documentation>
                  <xs:appinfo>
                     <WorkflowInformation xmlns="http://www.plan-pro.org/modell/PlanProInformation">
                        <Patternbeschreibung>[0.1..1.8]</Patternbeschreibung>
                     </WorkflowInformation>
                  </xs:appinfo>
               </xs:annotation>
            </xs:element>
            <xs:element name="Ader_Querschnitt"
                         type="nsMedien_und_Trassen:TCAder_Querschnitt"
                         minOccurs="0"
                         maxOccurs="1">
               <xs:annotation>
                  <xs:documentation>Bei Kabeln zur rein elektrischen Energieübertragung: Querschnitt der Kabeladern in Quadratmillimetern. Bsp.: 2x1,5.</xs:documentation>
                  <xs:appinfo>
                     <WorkflowInformation xmlns="http://www.plan-pro.org/modell/PlanProInformation">
                        <Patternbeschreibung>[1..99, optional eine Nachkommastelle]</Patternbeschreibung>
                     </WorkflowInformation>
                  </xs:appinfo>
               </xs:annotation>
            </xs:element>
         </xs:choice>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType ecore:name="Kabel_Typ_TypeClass" name="TCKabel_Typ">
      <xs:complexContent>
         <xs:extension base="nsBasisTypen:CBasisAttribut">
            <xs:sequence>
               <xs:element name="Wert"
                            type="nsMedien_und_Trassen:TKabel_Typ"
                            minOccurs="1"
                            maxOccurs="1"
                            nillable="true"/>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType ecore:name="Kabel_Bezeichnung_AttributeGroup" name="CKabel_Bezeichnung">
      <xs:sequence>
         <xs:element name="Bezeichnung_Kabel"
                      type="nsMedien_und_Trassen:TCBezeichnung_Kabel"
                      minOccurs="1"
                      maxOccurs="1">
            <xs:annotation>
               <xs:documentation>Bezeichnung des Kabels.</xs:documentation>
               <xs:appinfo>
                  <WorkflowInformation xmlns="http://www.plan-pro.org/modell/PlanProInformation">
                     <Patternbeschreibung>[1..16]</Patternbeschreibung>
                  </WorkflowInformation>
               </xs:appinfo>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType ecore:name="Trasse_Knoten_Art_TypeClass" name="TCTrasse_Knoten_Art">
      <xs:complexContent>
         <xs:extension base="nsBasisTypen:CBasisAttribut">
            <xs:sequence>
               <xs:element name="Wert"
                            type="nsMedien_und_Trassen:ENUMTrasse_Knoten_Art"
                            minOccurs="1"
                            maxOccurs="1"
                            nillable="true"/>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
