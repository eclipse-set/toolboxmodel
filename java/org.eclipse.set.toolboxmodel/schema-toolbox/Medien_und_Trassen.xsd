   <xs:complexType ecore:name="Induktionsschutz_TypeClass" name="TCInduktionsschutz">
      <xs:complexContent>
         <xs:extension base="nsBasisTypen:CBasisAttribut">
            <xs:sequence>
               <xs:element name="Wert"
                            type="nsMedien_und_Trassen:TInduktionsschutz"
                            minOccurs="1"
