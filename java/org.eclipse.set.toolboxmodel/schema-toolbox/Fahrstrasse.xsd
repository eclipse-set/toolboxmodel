   <xs:complexType ecore:name="Fstr_Mittel_V_Aufwertung_TypeClass"
                    name="TCFstr_Mittel_V_Aufwertung">
      <xs:complexContent>
         <xs:extension base="nsBasisTypen:CBasisAttribut">
