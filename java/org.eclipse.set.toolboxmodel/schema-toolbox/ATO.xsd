   <xs:complexType ecore:name="ATO_Timing_Point_Bezeichnung_AttributeGroup"
                    name="CATO_Timing_Point_Bezeichnung">
      <xs:sequence>
         <xs:element name="Bezeichnung_ATO_TP"
                      type="nsATO:TCBezeichnung_ATO_TP"
                      minOccurs="1"
                      maxOccurs="1">
            <xs:annotation>
               <xs:documentation>Fachliche Bezeichnung des Timing Points (Parameter „X_TEXT“ aus dem Block „Timing Point Name“, siehe Subset 126).</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType ecore:name="ATO_Timing_Point_Allg_AttributeGroup"
                    name="CATO_Timing_Point_Allg">
      <xs:sequence>
         <xs:element name="Erreichungstoleranz"
                      type="nsATO:TCErreichungstoleranz"
                      minOccurs="1"
                      maxOccurs="1">
            <xs:annotation>
               <xs:documentation>Kodierung des Haltefensters am Haltplatz bei manueller Fahrt. Der Parameter entspricht dem ATO-Parameter "Q_STP_Reached" gemäß Subset 126, Version 0.0.17 vom 05.08.2020.</xs:documentation>
               <xs:appinfo>
                  <WorkflowInformation xmlns="http://www.plan-pro.org/modell/PlanProInformation">
                     <Patternbeschreibung>[0..31]</Patternbeschreibung>
                  </WorkflowInformation>
               </xs:appinfo>
            </xs:annotation>
         </xs:element>
         <xs:element name="Haltetoleranz"
                      type="nsATO:TCHaltetoleranz"
                      minOccurs="1"
                      maxOccurs="1">
            <xs:annotation>
               <xs:documentation>Kodierung des Haltefensters am Halteplatz bei automatischer Fahrt (Q_Stop_Location Tolerance).</xs:documentation>
               <xs:appinfo>
                  <WorkflowInformation xmlns="http://www.plan-pro.org/modell/PlanProInformation">
                     <Patternbeschreibung>[0..31]</Patternbeschreibung>
                  </WorkflowInformation>
               </xs:appinfo>
            </xs:annotation>
         </xs:element>
         <xs:element name="NID_TP" type="nsATO:TCNID_TP" minOccurs="1" maxOccurs="1">
            <xs:annotation>
               <xs:documentation>Innerhalb eines Landes (NID-C-Bereich) einmalige Adresse des Timing Points.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType ecore:name="ATO_TS_Instanz_Adresse_AttributeGroup"
                    name="CATO_TS_Instanz_Adresse">
      <xs:sequence>
         <xs:element name="NID_ATOTS"
                      type="nsATO:TCNID_ATOTS"
                      minOccurs="1"
                      maxOccurs="1">
            <xs:annotation>
               <xs:documentation>Kennung der ATO-TS-Instanz als Bestandteil der ATO-Kennung (ATO-ID) gemäß Subset 126, Version 0.0.17 vom 05.08.2020.
Die Kennung wird zentral zugeteilt und verwaltet.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="NID_C_ATOTS"
                      type="nsATO:TCNID_C_ATOTS"
                      minOccurs="1"
                      maxOccurs="1">
            <xs:annotation>
               <xs:documentation>Gebietskennung als Bestandteil der ATO-Kennung (ATO-ID) gemäß Subset 126, Version 0.0.17 vom 05.08.2020.
Die Gebietskennung NID_C_ATOTS für ATO-TS- Instanzen wird aus der ETCS-Gebietskennung (NID_C) des entsprechenden Bereichs übernommen.</xs:documentation>
               <xs:appinfo>
                  <WorkflowInformation xmlns="http://www.plan-pro.org/modell/PlanProInformation">
                     <Patternbeschreibung>[0..1023]</Patternbeschreibung>
                  </WorkflowInformation>
               </xs:appinfo>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
