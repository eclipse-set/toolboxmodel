         <xs:element name="Max_Unterbrechungszeit"
                      type="nsBalisentechnik_ETCS:TCMax_Unterbrechungszeit"
                      minOccurs="0"
                      maxOccurs="1">
            <xs:annotation>
               <xs:documentation>Maximale Unterbrechungszeit für die Energieversorgung am Ausgang des Energieversorgungsmoduls.</xs:documentation>
               <xs:appinfo>
                  <WorkflowInformation xmlns="http://www.plan-pro.org/modell/PlanProInformation">
                     <Patternbeschreibung>[0.00..30.00]</Patternbeschreibung>
                  </WorkflowInformation>
               </xs:appinfo>
            </xs:annotation>
         </xs:element>
         <xs:element name="Nennleistung"
                      type="nsBalisentechnik_ETCS:TCNennleistung"
                      minOccurs="1"
