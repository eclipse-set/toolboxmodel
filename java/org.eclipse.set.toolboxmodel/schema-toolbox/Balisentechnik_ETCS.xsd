   <xs:complexType ecore:name="FT_Fahrweg_Teil" name="CFT_Fahrweg_Teil">
      <xs:annotation>
         <xs:documentation>Teil des Fahrwegs, auf den sich die Informationen des Fachtelegramms beziehen. Die Fahrwegteile können außerhalb der betroffenen Fahrstraße liegen. Im Unterschied zum Fahrweg der Fahrstraße wird der Fahrwegteil nicht als Bereichsobjekt abgebildet, da in einigen Fällen kein Zielelement existiert (z. B. Übergang in Rangierbereich) oder das exakte Ende des Fahrwegteils nicht bestimmt wird (Kreuzungsweichen). Fahrwegteile mit mehreren Zielpunkten sind durch mehrere Instanzen von FT_Fahrweg_Teil abzubilden.</xs:documentation>
         <xs:appinfo>
            <WorkflowInformation xmlns="http://www.plan-pro.org/modell/PlanProInformation">
               <ObjectType>LST_OBJECT</ObjectType>
               <Untergewerke>ETCS</Untergewerke>
            </WorkflowInformation>
         </xs:appinfo>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="nsBasisobjekte:CBasis_Objekt">
            <xs:sequence>
               <xs:element name="FT_Fahrweg_Teil_Allg"
                            type="nsBalisentechnik_ETCS:CFT_Fahrweg_Teil_Allg"
                            minOccurs="1"
                            maxOccurs="1"/>
               <xs:choice minOccurs="1" maxOccurs="1">
                  <xs:element name="ID_Ziel_Datenpunkt"
                               type="xs:anyURI"
                               ecore:reference="nsBalisentechnik_ETCS:CDatenpunkt"
                               nillable="true"
                               minOccurs="0"
                               maxOccurs="1">
                     <xs:annotation>
                        <xs:documentation>Verweis auf einen Datenpunkt, der als Ziel-Element zur Beschreibung des Fahrwegteils verwendet wird.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="ID_Ziel_Signal"
                               type="xs:anyURI"
                               ecore:reference="nsSignale:CSignal"
                               nillable="true"
                               minOccurs="0"
                               maxOccurs="1">
                     <xs:annotation>
                        <xs:documentation>Verweis auf ein Signal, das als Ziel-Element zur Beschreibung des Fahrwegteils verwendet wird.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="Ziel_W_Element"
                               type="nsBalisentechnik_ETCS:CZiel_W_Element"
                               minOccurs="0"
                               maxOccurs="1"/>
               </xs:choice>
               <xs:choice minOccurs="1" maxOccurs="1">
                  <xs:element name="ID_Start_Datenpunkt"
                               type="xs:anyURI"
                               ecore:reference="nsBalisentechnik_ETCS:CDatenpunkt"
                               nillable="true"
                               minOccurs="1"
                               maxOccurs="1">
                     <xs:annotation>
                        <xs:documentation>Verweis auf einen Datenpunkt, der als Start-Element zur Beschreibung des Fahrwegteils verwendet wird.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="ID_Start_Signal"
                               type="xs:anyURI"
                               ecore:reference="nsSignale:CSignal"
                               nillable="true"
                               minOccurs="1"
                               maxOccurs="1">
                     <xs:annotation>
                        <xs:documentation>Verweis auf ein Signal, das als Start-Element zur Beschreibung des Fahrwegteils verwendet wird.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="Start_W_Element"
                               type="nsBalisentechnik_ETCS:CStart_W_Element"
                               minOccurs="1"
                               maxOccurs="1"/>
               </xs:choice>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
