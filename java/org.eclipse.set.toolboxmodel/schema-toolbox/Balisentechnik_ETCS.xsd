   <xs:complexType ecore:name="Datenpunkt" name="CDatenpunkt">
      <xs:annotation>
         <xs:documentation>Beeinflussungspunkt bestehend aus einer Einzelbalise oder Balisengruppe und ggf. einer LEU. In europäischen Spezifikationen wird der Begriff "Balisengruppe" auch synonym für "Datenpunkt" verwendet. Ein ungesteuerter Datenpunkt besteht ausschließlich aus ungesteuerten Balisen (Festdatenbalisen). Die Attributgruppe DP_Typ_GESG darf nur ausgewählt werden, wenn ausschließlich ESG-Telegramme enthalten sind.</xs:documentation>
         <xs:appinfo>
            <WorkflowInformation xmlns="http://www.plan-pro.org/modell/PlanProInformation">
               <ObjectType>LST_OBJECT</ObjectType>
               <Untergewerke>ETCS</Untergewerke>
            </WorkflowInformation>
         </xs:appinfo>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="nsBasisobjekte:CPunkt_Objekt">
            <xs:sequence>
               <xs:element name="Datenpunkt_Allg"
                            type="nsBalisentechnik_ETCS:CDatenpunkt_Allg"
                            minOccurs="1"
                            maxOccurs="1"/>
               <xs:element name="Datenpunkt_Einmesspunkt"
                            type="nsBalisentechnik_ETCS:CDatenpunkt_Einmesspunkt"
                            minOccurs="0"
                            maxOccurs="1"/>
               <xs:element name="DP_Bezug_Funktional"
                            type="nsBalisentechnik_ETCS:CDP_Bezug_Funktional"
                            minOccurs="0"
                            maxOccurs="1"/>
               <xs:element name="DP_ETCS_Adresse"
                            type="nsBalisentechnik_ETCS:CDP_ETCS_Adresse"
                            minOccurs="0"
                            maxOccurs="1">
                  <xs:annotation>
                     <xs:documentation>Bei ESG optional (nur, wenn DP in mehreren Anwendungssystemen verwendet wird), bei allen anderen Anwendungssystemen Pflicht.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="DP_Typ"
                            type="nsBalisentechnik_ETCS:CDP_Typ"
                            minOccurs="1"
                            maxOccurs="unbounded"/>
               <xs:element name="ID_RBC"
                            type="xs:anyURI"
                            ecore:reference="nsBalisentechnik_ETCS:CRBC"
                            nillable="true"
                            minOccurs="0"
                            maxOccurs="unbounded">
                  <xs:annotation>
                     <xs:documentation>Verweis auf die RBC, denen der Datenpunkt bekannt ist.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="LEU_Steuernde"
                            type="nsBalisentechnik_ETCS:CLEU_Steuernde"
                            minOccurs="0"
                            maxOccurs="1"/>
               <xs:choice minOccurs="1" maxOccurs="1">
                  <xs:element name="DP_Telegramm"
                               type="nsBalisentechnik_ETCS:CDP_Telegramm"
                               minOccurs="1"
                               maxOccurs="unbounded"/>
                  <xs:element name="DP_Telegramm_ESG"
                               type="nsBalisentechnik_ETCS:CDP_Telegramm_ESG"
                               minOccurs="1"
                               maxOccurs="unbounded"/>
               </xs:choice>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType ecore:name="LEU_Schaltkasten_Bezeichnung_AttributeGroup"
                    name="CLEU_Schaltkasten_Bezeichnung">
