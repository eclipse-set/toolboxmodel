   <xs:complexType ecore:name="Fla_Schutz" name="CFla_Schutz">
      <xs:annotation>
         <xs:documentation>Darstellung der technischen Maßnahmen, um Flankenschutz zu gewährleisten. DB-Regelwerk 819.0505 </xs:documentation>
         <xs:appinfo>
            <WorkflowInformation xmlns="http://www.plan-pro.org/modell/PlanProInformation">
               <ObjectType>LST_OBJECT</ObjectType>
               <Untergewerke>ESTW</Untergewerke>
            </WorkflowInformation>
         </xs:appinfo>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="nsBasisobjekte:CBasis_Objekt">
            <xs:sequence>
               <xs:element name="Fla_Schutz_Anforderer"
                            type="nsFlankenschutz:CFla_Schutz_Anforderer"
                            minOccurs="1"
                            maxOccurs="1"/>
               <xs:choice minOccurs="1" maxOccurs="1">
                  <xs:element name="Fla_Schutz_Signal"
                               type="nsFlankenschutz:CFla_Schutz_Signal"
                               minOccurs="1"
                               maxOccurs="1"/>
                  <xs:element name="Fla_Schutz_W_Gsp"
                               type="nsFlankenschutz:CFla_Schutz_W_Gsp"
                               minOccurs="1"
                               maxOccurs="1"/>
                  <xs:element name="Fla_Schutz_Weitergabe"
                               type="nsFlankenschutz:CFla_Schutz_Weitergabe"
                               minOccurs="1"
                               maxOccurs="1"/>
                  <xs:element name="Fla_Verzicht"
                               type="nsFlankenschutz:TCFla_Verzicht"
                               minOccurs="1"
                               maxOccurs="1">
                     <xs:annotation>
                        <xs:documentation>Auswahl der Maßnahme "Verzicht" für den Flankenschutz. Der Flankenschutz wird durch betriebliche Maßnahmen hergestellt (mittelbarer Flankenschutz). Die Maßnahmen "unmittelbar" und "Weitergabe" sind aus der Befüllung anderer Attribute erkennbar, deshalb muss nur die Maßnahme "Verzicht" explizit angegeben werden. DB-Regelwerk Flankenschutztabelle, Spalte 8 "Flankenschutzumkehr, Verzicht"</xs:documentation>
                     </xs:annotation>
                  </xs:element>
               </xs:choice>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
