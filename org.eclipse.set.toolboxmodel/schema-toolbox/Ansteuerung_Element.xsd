   <xs:complexType ecore:name="Uebertragungsweg_Technik_AttributeGroup"
                    name="CUebertragungsweg_Technik">
      <xs:sequence>
         <xs:element name="Bandbreite"
                      type="nsAnsteuerung_Element:TCBandbreite"
                      minOccurs="0"
                      maxOccurs="1">
            <xs:annotation>
               <xs:documentation>Beschreibt die nötige Bandbreite des Übertragungsweges. Beispiel: 2Mbit_s bei UZ-BZ-Verbindung.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Medium_Art"
                      type="nsAnsteuerung_Element:TCMedium_Art"
                      minOccurs="0"
                      maxOccurs="1">
            <xs:annotation>
               <xs:documentation>Beschreibt den Übergabepunkt des Übertragungsweges in dessen physikalischer Ausprägung. Beispiel: ADO8-Dose bei ZBS-Verbindung, RJ45-Port bei UZ-BZ-Verbindung.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Netz_Art"
                      type="nsAnsteuerung_Element:TCNetz_Art"
                      minOccurs="0"
                      maxOccurs="1">
            <xs:annotation>
               <xs:documentation>Beschreibt die Art des Übertragungsnetzes, ggf. auch die Anforderungen. Beispiel: geschlossenes Netz oder offenes Netz bei der ZBS-Verbindung.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Technik_Art"
                      type="nsAnsteuerung_Element:TCTechnik_Art"
                      minOccurs="1"
                      maxOccurs="1">
            <xs:annotation>
               <xs:documentation>Technische Ausprägung der physikalischen Schnittstelle. Beispiel: G.703, Ethernet oder LWL_Faser.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Technik_Beschreibung"
                      type="nsAnsteuerung_Element:TCTechnik_Beschreibung"
                      minOccurs="0"
                      maxOccurs="1">
            <xs:annotation>
               <xs:documentation>Beschreibung der technischen Realisierung des Übertragungsweges in Textform.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
