   <xs:complexType ecore:name="Technik_Standort" name="CTechnik_Standort">
      <xs:annotation>
         <xs:documentation>Bei DSTW: Bündelung der technischen Anlagen für ESTW_Zentraleinheit, Telekommunikation, Prozessdaten- und Diagnoseschnittstellen sowie die zugehörige technische Gebäudeausrüstung. Je Netzbezirk ist ein Technikstandort vorgesehen.</xs:documentation>
         <xs:appinfo>
            <WorkflowInformation xmlns="http://www.plan-pro.org/modell/PlanProInformation">
               <ObjectType>LST_OBJECT</ObjectType>
            </WorkflowInformation>
         </xs:appinfo>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="nsBasisobjekte:CBasis_Objekt">
            <xs:sequence>
               <xs:element name="Bezeichnung"
                            type="nsAnsteuerung_Element:CTechnik_Standort_Bezeichnung"
                            minOccurs="1"
                            maxOccurs="1"/>
               <xs:element name="ID_Bedien_Standort"
                            type="xs:anyURI"
                            ecore:reference="nsBedienung:CBedien_Standort"
                            nillable="true"
                            minOccurs="0"
                            maxOccurs="unbounded">
                  <xs:annotation>
                     <xs:documentation>Verweis auf den Bedien-Standort, von dem aus der Technik-Standort bedient wird. Der Verweis dient der Abbildung der vorgesehenen n zu m-Zuordnung zwischen TSO und BSO. Die Angabe erfolgt im Rahmen der Planung des Untergewerks Bedienung Fdl.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="ID_Unterbringung"
                            type="xs:anyURI"
                            ecore:reference="nsAnsteuerung_Element:CUnterbringung"
                            nillable="true"
                            minOccurs="1"
                            maxOccurs="1">
                  <xs:annotation>
                     <xs:documentation>Verweis auf die Unterbringung (Gebäude), in der der Technik-Standort untergebracht ist.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="TSO_IP_Adressblock"
                            type="nsAnsteuerung_Element:CTSO_IP_Adressblock"
                            minOccurs="0"
                            maxOccurs="1"/>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
