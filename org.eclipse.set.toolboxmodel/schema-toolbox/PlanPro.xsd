                            maxOccurs="1"/>
               <xs:element name="Kontaktdaten"
                            type="nsPlanPro:COrganisation"
                            minOccurs="1"
                            maxOccurs="1">
                  <xs:annotation>
                     <xs:documentation>Kontaktdaten des Akteurs zur Unternehmenszuordnung.
DB-Regelwerk
Bisher keine Abbildung als Schriftfeldeintrag gemäß Ril 819.0103.</xs:documentation>
                  </xs:annotation>
